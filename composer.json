{
    "name": "matta-davi/laravel-api-model",
    "description": "API driver for Laravel Eloquent models",
    "keywords": [
        "laravel",
        "JSON api",
        "api",
        "eloquent"
    ],
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0",
        "nesbot/carbon": "^2.61"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Matta",
            "email": "matta.davi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MattaDavi\\LaravelApiModel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattaDavi\\LaravelApiModel\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
