{
    "name": "friendsofcat/laravel-api-model",
    "description": "API driver for Laravel Eloquent models",
    "keywords": [
        "laravel",
        "JSON api",
        "api",
        "eloquent"
    ],
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^7.0 || ^9.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FriendsOfCat\\LaravelApiModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FriendsOfCat\\LaravelApiModel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
